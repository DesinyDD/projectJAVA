<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="packman.controllers.officerMenu.mailRoomMenu.MailRoomController">
   <children>
      <Button fx:id="addMailButton" layoutX="210.0" layoutY="442.0" mnemonicParsing="false" onAction="#handleAddMailButtonOnAction" prefHeight="50.0" prefWidth="200.0" text="ADD MAIL" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">
