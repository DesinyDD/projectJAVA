      <Button fx:id="checkOutButton" layoutX="10.0" layoutY="442.0" mnemonicParsing="false" onAction="#handleCheckOutButtonOInAction" prefHeight="50.0" prefWidth="200.0" text="CHECK OUT" AnchorPane.bottomAnchor="220.0" AnchorPane.rightAnchor="10.0">
         <font>
            <Font name="Arial Rounded MT Bold" size="20.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="1116.0" layoutY="656.0" mnemonicParsing="false" onAction="#handleBackButtonOnAction" prefHeight="50.0" prefWidth="200.0" text="BACK" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0">
