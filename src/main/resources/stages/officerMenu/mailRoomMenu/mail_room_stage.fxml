            <Font size="16.0" />
         </font>
      </TextField>
      <TableView fx:id="mailsTable" layoutX="27.0" layoutY="303.0" prefHeight="360.0" prefWidth="1280.0" AnchorPane.bottomAnchor="280.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="70.0" />
